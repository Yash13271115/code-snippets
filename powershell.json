	"PowerShell Function": {
			"prefix": "func",
			"body": [
				"function ${1:FunctionName} {",
				"\tparam(",
				"\t\t[Parameter(Mandatory = $true)]",
				"\t\t[string]$${2:ParamName}",
				"\t)",
				"",
				"\t${3:Write-Output \"Hello from function!\"}",
				"}",
				"$0"
			],
			"description": "A basic PowerShell function template"
	}
}