    },
    "Blade for loop": {
        "prefix": "bfor",
        "body": [
            "@for($${1:i} = 0; $${1:i} < $${2:count}; $${1:i}++)",
            "    $0",
            "@endfor"
        ],
        "description": "Laravel Blade for loop"
    }
}